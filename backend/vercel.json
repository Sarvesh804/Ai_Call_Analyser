{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/data",
        "dest": "/api/data.js"
      },
      {
        "src": "/send-welcome-email",
        "dest": "/api/send-welcome-email.js"
      }
    ]
  }
  